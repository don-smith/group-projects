{
  "name": "group-projects",
  "version": "1.0.0",
  "description": "Capturing the awesomeness that is EDA group projects",
  "main": "server.js",
  "scripts": {
    "debug": "node-inspector & node-dev --debug server.js",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "NODE_ENV=dev knex migrate:latest",
    "migrate:rollback": "NODE_ENV=dev knex migrate:rollback",
    "seed:make": "knex seed:make",
    "seed:run": "NODE_ENV=dev knex seed:run",
    "start": "node server.js",
    "test": "tape tests/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/locksmithdon/group-projects.git"
  },
  "keywords": [
    "react",
    "node",
    "express",
    "knex"
  ],
  "author": "Don Smith <locksmithdon@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/locksmithdon/group-projects/issues"
  },
  "homepage": "https://github.com/locksmithdon/group-projects#readme",
  "dependencies": {
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "knex": "^0.11.5",
    "pg": "^4.5.6",
    "sqlite3": "^3.1.4"
  },
  "devDependencies": {
    "node-dev": "^3.1.3",
    "node-inspector": "^0.12.8",
    "supertest": "^1.2.0",
    "tape": "^4.5.1"
  }
}
